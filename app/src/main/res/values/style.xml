<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Button.White" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">@color/white</item>
    </style>
    <style name="BottomNavigationViewTextStyle" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:fontFamily">@font/roboto_medium</item>
    </style>
    <style name="ItemSelected" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="color">@color/purple_200</item>
    </style>
    <style name="SearchView" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:textColorHint">#56000000</item>
        <item name="android:editTextColor">@android:color/black</item>
    </style>

    <style name="Cardview" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:textColorHint">@color/landingText</item>
        <item name="android:editTextColor">@android:color/black</item>
        <item name="android:radius">25dp</item>
    </style>

</resources>